{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "quotes": ["warn","double"],
    "jsx-quotes": ["warn", "prefer-double"],
    "indent": ["warn", 4],
    "max-len": ["warn", {"code": 180}],
    "comma-dangle": ["warn", "always-multiline"]
//    "semi": ["warn", "always"]
  }
}